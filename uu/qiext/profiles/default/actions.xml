<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object" meta_type="CMF Action Category">
  <property name="title"></property>
  <object insert-before="contentrules" name="workspace_membership" meta_type="CMF Action">
   <property name="title">Membership</property>
   <property
      name="description">Workspace membership for types providing IWorkspace.</property>
   <property
      name="url_expr">string:${object_url}/@@workspace_membership</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:here.portal_interface.objectImplements(here, 'Products.qi.extranet.types.interfaces.IWorkspace')</property>
   <property name="permissions">
    <element value="Manage users"/>
   </property>
   <property name="visible">True</property>
  </object>

  <object insert-before="contentrules" name="workspace_roster" meta_type="CMF Action">
   <property name="title">Roster</property>
   <property
      name="description">Workspace roster for types providing IWorkspace.</property>
   <property
      name="url_expr">string:${object_url}/@@roster</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:here.portal_interface.objectImplements(here, 'Products.qi.extranet.types.interfaces.IWorkspace')</property>
   <property name="visible">True</property>
  </object>
 </object>
</object>

